[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
req_extensions     = req_ext
x509_extensions    = v3_ca
prompt             = no

[ req_distinguished_name ]
CN = erag.com

[ req_ext ]
subjectAltName = @alt_names

[ v3_ca ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = erag.com
DNS.2 = auth.erag.com
DNS.3 = grafana.erag.com
DNS.4 = apisix.erag.com
DNS.5 = s3.erag.com
DNS.6 = minio.erag.com
